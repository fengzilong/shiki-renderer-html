{
  "root": true,
  "extends": [
    "mo",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-duplicate-imports": "off",
    "@typescript-eslint/no-duplicate-imports": [
      "error"
    ]
  }
}